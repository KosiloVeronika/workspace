package com.epam.libraryManager.entity;

public class Author {
	private int id;
	private String name;
	private String surname;
	private String secondName;
	public Author(int id, String name, String surname, String secondName) {
		this.id = id;
		this.name = name;
		this.surname = surname;
		this.secondName = secondName;
	}

}
